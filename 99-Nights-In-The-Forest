-- AUTO FIRE (—Ç–µ–ª–µ–ø–æ—Ä—Ç –≤ –∫–æ—Å—Ç—ë—Ä)
-- ========================

local FIRE_CHOICES = { "Cultist", "Crossbow Cultist", "Fuel Canister", "Coal", "Chair", "Log", "Oil Barrel" }
local fireSelected = { "Fuel Canister", "Coal", "Log" } -- –¥–µ—Ñ–æ–ª—Ç –ø–æ–±–µ–∑–æ–ø–∞—Å–Ω–µ–µ
local fireSelectedSet = toSet(fireSelected)
local sntAutoFire = false

AutoFarmTab:Paragraph({
    Title = gradient("AUTO FIRE", COLOR_PALETTE.Primary, COLOR_PALETTE.Text),
    Desc  = "–¢–µ–ª–µ–ø–æ—Ä—Ç –≤—ã–±—Ä–∞–Ω–Ω—ã—Ö –ø—Ä–µ–¥–º–µ—Ç–æ–≤ –≤ –∫–æ—Å—Ç—ë—Ä (MainFire)",
    Image = "flame", ImageSize = 18, Color = "White"
})

AutoFarmTab:Dropdown({
    Title = "Fire Targets (Multi)",
    Values = FIRE_CHOICES,
    Value  = fireSelected,
    Multi  = true,
    AllowNone = true,
    Callback = function(choice)
        fireSelected = ensureTableChoice(choice)
        fireSelectedSet = toSet(fireSelected)
        WindUI:Notify({ Title="Auto Fire", Content="–í—ã–±—Ä–∞–Ω–æ: "..tostring(#fireSelected).." –ø—Ä–µ–¥–º–µ—Ç–æ–≤", Duration=1 })
    end
})

local function fireLoop()
    while sntAutoFire do
        local fireCf = mainFireCFrame()
        -- –ò—â–µ–º –≤ Items
        local items = scanFolderForNames(Workspace:FindFirstChild("Items"), fireSelectedSet)
        for _, m in ipairs(items) do
            if not sntAutoFire then break end
            pivotTo(m, jitterAround(fireCf, 0.4))
            task.wait(0.05)
        end
        -- –î–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω–æ: –Ω–µ–∫–æ—Ç–æ—Ä—ã–µ —Ü–µ–ª–∏ –º–æ–≥—É—Ç –±—ã—Ç—å –≤ Characters (Cultist)
        local chars = scanFolderForNames(Workspace:FindFirstChild("Characters"), fireSelectedSet)
        for _, m in ipairs(chars) do
            if not sntAutoFire then break end
            pivotTo(m, jitterAround(fireCf, 0.4))
            task.wait(0.05)
        end
        task.wait(0.7)
    end
end

AutoFarmTab:Toggle({
    Title = gradient("Auto Fire", COLOR_PALETTE.Primary, COLOR_PALETTE.Text),
    Desc  = "–¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä—É–µ—Ç –≤—ã–±—Ä–∞–Ω–Ω—ã–µ —Ü–µ–ª–∏ –≤ –∫–æ—Å—Ç—ë—Ä",
    Value = sntAutoFire,
    Callback = function(state)
        sntAutoFire = state
        if state then
            task.spawn(fireLoop)
            WindUI:Notify({ Title="Auto Fire", Content="–ó–∞–ø—É—â–µ–Ω–æ", Duration=2 })
        else
            WindUI:Notify({ Title="Auto Fire", Content="–û—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–æ", Duration=2 })
        end
    end
})

