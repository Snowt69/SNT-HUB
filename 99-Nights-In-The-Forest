    Title = "Cook Targets (Multi, one-shot)",
    Values = COOK_CHOICES,
    Value  = cookSelected,
    Multi  = true,
    AllowNone = true,
    Callback = function(choice)
        cookSelected = ensureTableChoice(choice)
        cookSelectedSet = toSet(cookSelected)
    end
})

AutoFarmTab:Button({
    Title = gradient("Cook Selected Once", COLOR_PALETTE.Primary, COLOR_PALETTE.Accent),
    Icon  = "flame",
    Callback = function()
        local fireCf = mainFireCFrame()
        local found = {}
        -- –ü–æ–∏—â–µ–º —Å—Ä–∞–∑—É –≤—Å–µ –≤—ã–±—Ä–∞–Ω–Ω—ã–µ
        local itemsFolder = Workspace:FindFirstChild("Items")
        if itemsFolder then
            for _, m in ipairs(itemsFolder:GetChildren()) do
                if m:IsA("Model") and cookSelectedSet[m.Name] then
                    table.insert(found, m)
                end
            end
        end
        local dropped = 0
        for _, item in ipairs(found) do
            pivotTo(item, jitterAround(fireCf, 0.35))
            dropped += 1
            task.wait(0.04)
        end
        WindUI:Notify({
            Title = "Auto Cook",
            Content = ("–û—Ç–ø—Ä–∞–≤–ª–µ–Ω–æ –∫ –∫–æ—Å—Ç—Ä—É: %d"):format(dropped),
            Duration = 2
        })
    end
})

-- ========================
-- AUTO EAT (—Å –º—É–ª—å—Ç–∏–≤—ã–±–æ—Ä–æ–º)
-- ========================

local EAT_CHOICES = { "Cake", "Carrot", "Apple", "Cooked Steak", "Cooked Morsel" }
local eatSelected = { "Cooked Steak", "Cooked Morsel", "Carrot", "Apple", "Cake" }
local eatSelectedSet = toSet(eatSelected)
local sntAutoEat = false

AutoFarmTab:Paragraph({
    Title = gradient("AUTO EAT", COLOR_PALETTE.Primary, COLOR_PALETTE.Text),
    Desc  = "–ï—Å—Ç –≤—ã–±—Ä–∞–Ω–Ω—ã–µ –ø—Ä–æ–¥—É–∫—Ç—ã: —Å–Ω–∞—á–∞–ª–∞ –∏–∑ ItemBag, –∑–∞—Ç–µ–º getNil, –∑–∞—Ç–µ–º –∏–∑ –º–∏—Ä–∞",
    Image = "utensils", ImageSize = 18, Color = "White"
})

AutoFarmTab:Dropdown({
    Title = "Food (Multi)",
    Values = EAT_CHOICES,
    Value  = eatSelected,
    Multi  = true,
    AllowNone = true,
    Callback = function(choice)
        eatSelected = ensureTableChoice(choice)
        eatSelectedSet = toSet(eatSelected)
        WindUI:Notify({ Title="Auto Eat", Content="–í—ã–±—Ä–∞–Ω–æ: "..tostring(#eatSelected).." –ø—Ä–æ–¥—É–∫—Ç–æ–≤", Duration=1 })
    end
})

local EAT_PRIORITY = { "Cooked Steak", "Cooked Morsel", "Cake", "Carrot", "Apple" }

local function consumeByName(nm)
    -- 1) –ò–∑ ItemBag
    local bag = LP:FindFirstChild("ItemBag")
    if bag then
        local it = bag:FindFirstChild(nm)
        if it then
            local ok = pcall(function()
                Remotes:WaitForChild("RequestConsumeItem"):InvokeServer(it)
            end)
            if ok then return true end
        end
    end
    -- 2) getNil
    local ghost = (typeof(getNil)=="function") and getNil(nm, "Model") or nil
    if ghost then
        local ok = pcall(function()
            Remotes:WaitForChild("RequestConsumeItem"):InvokeServer(ghost)
        end)
        if ok then return true end
    end
    -- 3) –ò–∑ –º–∏—Ä–∞ (Items) + —Ç–µ–ª–µ–ø–æ—Ä—Ç –∫ —Å–µ–±–µ –ø—Ä–∏ –Ω–µ–æ–±—Ö–æ–¥–∏–º–æ—Å—Ç–∏
    local itemsFolder = Workspace:FindFirstChild("Items")
    if itemsFolder then
        for _, m in ipairs(itemsFolder:GetChildren()) do
            if m:IsA("Model") and string.lower(m.Name) == string.lower(nm) then
                -- –ø—Ä—è–º–∞—è –ø–æ–ø—ã—Ç–∫–∞ —Å—ä–µ—Å—Ç—å –∏–∑ –º–∏—Ä–∞
                local ok = pcall(function()
                    Remotes:WaitForChild("RequestConsumeItem"):InvokeServer(m)
                end)
                if ok then return true end
                -- –µ—Å–ª–∏ –Ω–µ –≤—ã—à–ª–æ, –ø–æ–¥—Ç—è–Ω–µ–º –∫ —Å–µ–±–µ –∏ –ø–æ–≤—Ç–æ—Ä–∏–º
                bringToPlayer(m, 2.5)
                task.wait(0.05)
                pcall(function()
                    Remotes:WaitForChild("RequestConsumeItem"):InvokeServer(m)
                end)
                return true
            end
        end
    end
    return false
end

local function eatLoop()
    while sntAutoEat do
        local ateSomething = false
        -- –∏–¥—ë–º –ø–æ –ø—Ä–∏–æ—Ä–∏—Ç–µ—Ç—É, –Ω–æ —É—á–∏—Ç—ã–≤–∞–µ–º —Ç–æ–ª—å–∫–æ –≤—ã–±—Ä–∞–Ω–Ω—ã–µ
        for _, nm in ipairs(EAT_PRIORITY) do
            if not sntAutoEat then break end
            if eatSelectedSet[nm] then
                local ok = consumeByName(nm)
                if ok then
                    ateSomething = true
                    -- –Ω–µ–±–æ–ª—å—à–∞—è –ø–∞—É–∑–∞, —á—Ç–æ–±—ã —Å–µ—Ä–≤–µ—Ä –ø—Ä–∏–Ω—è–ª
                    task.wait(0.2)
                end
            end
        end
        if not ateSomething then
            -- –Ω–∏—á–µ–≥–æ –∏–∑ —Å–ø–∏—Å–∫–∞ –Ω–µ –Ω–∞—à–ª–∏ ‚Äî –ø–æ–¥–æ–∂–¥—ë–º –∏ –ø–æ–≤—Ç–æ—Ä–∏–º
            task.wait(2.0)
        else
            -- —Å—ä–µ–ª–∏ —á—Ç–æ-—Ç–æ ‚Äî –µ—â—ë –æ–¥–Ω–∞ –∫–æ—Ä–æ—Ç–∫–∞—è –∏—Ç–µ—Ä–∞—Ü–∏—è
            task.wait(0.8)
        end
    end
end

AutoFarmTab:Toggle({
    Title = gradient("Auto Eat", COLOR_PALETTE.Primary, COLOR_PALETTE.Text),
    Desc  = "–ï—Å—Ç –≤—ã–±—Ä–∞–Ω–Ω—ã–µ –ø—Ä–æ–¥—É–∫—Ç—ã –ø–æ –ø—Ä–∏–æ—Ä–∏—Ç–µ—Ç—É",
    Value = sntAutoEat,
    Callback = function(state)
        sntAutoEat = state
        if state then
            task.spawn(eatLoop)
            WindUI:Notify({ Title="Auto Eat", Content="–ó–∞–ø—É—â–µ–Ω–æ", Duration=2 })
        else
            WindUI:Notify({ Title="Auto Eat", Content="–û—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–æ", Duration=2 })
        end
    end
})

AutoFarmTab:Button({
    Title = "Eat Selected Now (once)",
    Icon  = "play",
    Callback = function()
        local ate = 0
        for _, nm in ipairs(EAT_PRIORITY) do
            if eatSelectedSet[nm] then
                if consumeByName(nm) then ate += 1 end
            end
        end
        WindUI:Notify({ Title="Eat Now", Content=("–°—ä–µ–¥–µ–Ω–æ –ø–æ–ø—ã—Ç–æ–∫: %d"):format(ate), Duration=2 })
    end
})

-- ========================
-- STOP ALL
-- ========================

AutoFarmTab:Button({
    Title   = gradient("Stop All Auto Farms", COLOR_PALETTE.Primary, COLOR_PALETTE.Danger),
    Icon    = "octagon",
    Callback = function()
        sntAutoWood = false
        sntAutoFire = false
        sntAutoScrap = false
        sntAutoEat  = false
        WindUI:Notify({ Title="Auto Farm", Content="–í—Å–µ –ø—Ä–æ—Ü–µ—Å—Å—ã –æ—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω—ã", Duration=2 })
    end
})
