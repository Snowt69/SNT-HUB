    Title = gradient("AUTO COOK (ONE-SHOT)", COLOR_PALETTE.Primary, COLOR_PALETTE.Text),
    Desc  = "–†–∞–∑–æ–≤–∞—è –æ—Ç–ø—Ä–∞–≤–∫–∞ –≤—ã–±—Ä–∞–Ω–Ω—ã—Ö —Å—ã—Ä—ã—Ö –ø—Ä–æ–¥—É–∫—Ç–æ–≤ –≤ –∫–æ—Å—Ç—ë—Ä",
    Image = "chef-hat", ImageSize = 18, Color = "White"
})

