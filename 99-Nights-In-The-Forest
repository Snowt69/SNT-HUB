-- ========================
-- –†–ï–ê–õ–ò–ó–ê–¶–ò–Ø AUTOFARM TAB (FULL REWRITE)
-- ========================

-- –õ–æ–∫–∞–ª—å–Ω—ã–µ —É—Ç–∏–ª–∏—Ç—ã
local LP = game:GetService("Players").LocalPlayer
local Workspace = game:GetService("Workspace")
local Remotes = game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents")

local function isAliveModel(m)
    return m and m.Parent ~= nil
end

local function safeDescendantOf(inst, ancestor)
    if not inst then return false end
    local ok = pcall(function() return inst:IsDescendantOf(ancestor) end)
    return ok and inst:IsDescendantOf(ancestor)
end

local function ensureTableChoice(choice)
    if typeof(choice) == "table" then return choice end
    if type(choice) == "string" then return { choice } end
    return {}
end

local function toSet(list)
    local s = {}
    for _, v in ipairs(list or {}) do s[v] = true end
    return s
end

local function scanFolderForNames(folder, namesSet)
    local results = {}
    if not folder then return results end
    for _, m in ipairs(folder:GetChildren()) do
        if m:IsA("Model") and namesSet[m.Name] then
            table.insert(results, m)
        end
    end
    return results
end

local function jitterAround(cf, r)
    r = r or 0.4
    return cf * CFrame.new(math.random(-100,100)/100 * r, 0, math.random(-100,100)/100 * r)
end

-- ========================
-- –î–ï–†–ï–í–¨–Ø (AUTO WOOD)
-- ========================

local TREE_TYPES = { "Small Tree", "TreeBig", "TreeBig2", "TreeBig3" }
local sntTreeType = TREE_TYPES[1]
local sntAutoWood = false
local TREE_DAMAGE_KEY = "3_9130026089"

AutoFarmTab:Paragraph({
    Title = gradient("WOOD FARMING", COLOR_PALETTE.Primary, COLOR_PALETTE.Text),
    Desc  = "–†—É–±–∏—Ç –≤—ã–±—Ä–∞–Ω–Ω—ã–π —Ç–∏–ø –¥–µ—Ä–µ–≤–∞, –ø–æ–∫–∞ –º–æ–¥–µ–ª—å –Ω–µ –∏—Å—á–µ–∑–Ω–µ—Ç",
    Image = "tree", ImageSize = 18, Color = "White"
})

AutoFarmTab:Dropdown({
    Title = "Tree Type",
    Values = TREE_TYPES,
    Value  = sntTreeType,
    Multi = false,
    AllowNone = false,
    Callback = function(v)
        sntTreeType = v or TREE_TYPES[1]
    end
})

local function getAllTreesByType(kind)
    local res = {}
    local fol = Workspace:FindFirstChild("Map"); fol = fol and fol:FindFirstChild("Foliage")
    if not fol then return res end
    local look = string.lower(kind)
    for _, node in ipairs(fol:GetChildren()) do
        if node:IsA("Model") then
            local ln = string.lower(node.Name)
            if string.find(ln, look, 1, true) then
                table.insert(res, node)
            end
        end
    end
    return res
end

local function chopTreeUntilGone(tree)
    local axe = getBestAxe and getBestAxe() or nil
    if not axe then
        WindUI:Notify({ Title = "Auto Wood", Content = "–¢–æ–ø–æ—Ä –Ω–µ –Ω–∞–π–¥–µ–Ω –≤ Inventory", Duration = 2 })
        return
    end
    while sntAutoWood and isAliveModel(tree) and safeDescendantOf(tree, Workspace) do
        local cf = (HRP() and HRP().CFrame) or CFrame.new()
        pcall(function()
            Remotes:WaitForChild("ToolDamageObject"):InvokeServer(tree, axe, TREE_DAMAGE_KEY, cf)
        end)
        task.wait(0.12)
    end
end

local function woodLoop()
    while sntAutoWood do
        local batch = getAllTreesByType(sntTreeType)
        if #batch == 0 then
            task.wait(1.2)
        else
            for _, tree in ipairs(batch) do
                if not sntAutoWood then break end
                chopTreeUntilGone(tree)
                task.wait(0.05)
            end
        end
        task.wait(0.4)
    end
end

AutoFarmTab:Toggle({
    Title = gradient("Auto Wood Farm", COLOR_PALETTE.Primary, COLOR_PALETTE.Text),
    Desc  = "–£–¥–∞–ª—ë–Ω–Ω–∞—è —Ä—É–±–∫–∞ —á–µ—Ä–µ–∑ Remote –¥–æ –∏—Å—á–µ–∑–Ω–æ–≤–µ–Ω–∏—è –¥–µ—Ä–µ–≤–∞",
    Value = sntAutoWood,
    Callback = function(state)
        sntAutoWood = state
        if state then
            task.spawn(woodLoop)
            WindUI:Notify({ Title="Auto Wood", Content="–ó–∞–ø—É—â–µ–Ω–æ", Duration=2 })
        else
            WindUI:Notify({ Title="Auto Wood", Content="–û—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–æ", Duration=2 })
        end
    end
})

